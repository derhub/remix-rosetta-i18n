{
  "name": "remix-rosetta-i18n",
  "version": "0.0.0",
  "description": "Lightweight i18n solution for your remix app",
  "main": "build/cjs/index.js",
  "module": "build/mjs/index.js",
  "exports": {
    ".": {
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:cjs & npm run build:mjs",
    "build:cjs": "tsc --module commonjs --outDir build/cjs",
    "build:mjs": "tsc --module esnext --outDir build/mjs",
    "typecheck:": "tsc --noEmit",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derhub/remix-rosetta-i18n.git"
  },
    "bugs": {
    "url": "https://github.com/derhub/remix-rosetta-i18n/issues"
  },
  "homepage": "https://github.com/derhub/remix-rosetta-i18n#readme",
  "keywords": [
    "remix",
    "i18n",
    "rosetta",
    "translation"
  ],
  "author": {
    "name": "Johnder Baul",
    "email": "imjohnderbaul@gmail.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "rosetta": "^1.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/prettier": "2.4.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.4.6",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-environment-jsdom": "^27.4.6",
    "lint-staged": "^12.3.1",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
